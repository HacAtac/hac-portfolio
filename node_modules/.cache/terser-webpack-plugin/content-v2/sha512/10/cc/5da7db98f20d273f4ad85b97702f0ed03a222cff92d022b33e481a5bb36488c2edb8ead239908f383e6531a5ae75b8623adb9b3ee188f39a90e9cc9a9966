age to the page they clicked on\n\n  return (\n    <nav id=\"nav-fix\">\n      <ul className=\"flex-row mobile-view mt-3 \">\n        <li className={currentPage === \"about\" ? \"mx-2 navActive\" : \"mx-2\"}>\n          <span onClick={() => setCurrentPage(\"about\")}>About Me</span>\n        </li>\n        <li className={currentPage === \"portfolio\" ? \"mx-2 navActive\" : \"mx-2\"}>\n          <span onClick={() => setCurrentPage(\"portfolio\")}>Portfolio</span>\n        </li>\n        <li className={currentPage === \"contact\" ? \"mx-2 navActive\" : \"mx-2\"}>\n          <span onClick={() => setCurrentPage(\"contact\")}>Contact</span>\n        </li>\n        <li className={currentPage === \"resume\" ? \"mx-2 navActive\" : \"mx-2\"}>\n          <span onClick={() => setCurrentPage(\"resume\")}>Resume</span>\n        </li>\n      </ul>\n    </nav>\n  );\n}\n\nexport default Nav;\n","import React from \"react\";\nimport Nav from \"../Nav\";\n// import Nav from \"../Nav\";\n\nfunction Header(props) {\n  const { currentPage, setCurrentPage } = props;\n\n  return (\n    <header className=\"flex flex-wrap justify-between px-1 header\">\n      <h2>\n        <a href=\"/\" className=\"navActive\" id=\"name-style\">\n          <span id=\"icon\" role=\"img\" aria-label=\"icon\" className=\"navActive\">\n            🖥️\n          </span>{\" \"}\n          Jordan Hackworth\n        </a>\n      </h2>\n      <div>\n        <Nav currentPage={currentPage} setCurrentPage={setCurrentPage}></Nav>\n      </div>\n    </header>\n  );\n}\n\nexport default Header;\n","import React from \"react\";\n\nexport default function Footer() {\n  //need to have a footer that will center three icons for github, linkedin, and stackoverflow\n  return (\n    <div className=\"flex justify-center\">\n      <a\n        href=\"https://github.com/HacAtac \"\n        target=\"_blank\"\n        rel=\"noopener noreferrer\"\n      >\n        <img\n          id=\"icon-animate\"\n          className=\"animate-bounce